{
  "name": "vue3-vitejs-ts-template",
  "version": "0.0.0",
  "description": "Vue 3 + Typescript + Vite with curated libs, based on `yarn create vite`",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@vueuse/core": "^5.2.0",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.25.0",
    "eslint-plugin-vue": "^7.9.0",
    "typescript": "^4.3.2",
    "vite": "^2.4.3",
    "vite-plugin-windicss": "^1.2.5",
    "vue-tsc": "^0.0.24",
    "windicss": "^3.1.5"
  },
  "keywords": [
    "vite",
    "vue",
    "vitejs",
    "vue3",
    "typescript"
  ]
}
